{
  "include": [
    "./lib"
  ],
  "compilerOptions": {
    "module": "es2020",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "removeComments": false,
    "outDir": "./build/esm/",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "ES2021"
  }
}
